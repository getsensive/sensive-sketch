{
  "name": "sensive-sketch",
  "description": "Sync your .sketch documents with your Sensive account.",
  "repository": "git@github.com:sensive/sensive-sketch.git",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sensive-sketch",
    "manifest": "src/manifest.json",
    "main": "sensive-sketch.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "SENSIVE_API_URL=https://sensive.co skpm-build",
    "build:staging": "SENSIVE_API_URL=https://staging.sensive.co skpm-build",
    "watch": "SENSIVE_API_URL=http://localhost:5000 skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.10"
  },
  "dependencies": {
    "sketch-module-web-view": "^1.2.3",
    "skpm": "^1.1.8"
  },
  "version": "0.3.0"
}
